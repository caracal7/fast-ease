{
  "name": "move.js",
  "version": "0.0.0",
  "description": "Fast and minimalistic javascript animation library",
  "main": "dist/move.cjs.js",
  "browser": "dist/move.esm.js",
  "type": "module",
  "scripts": {
    "start": "run-p dev:*",
    "dev:build_esm": "./node_modules/.bin/esbuild src/index.js --format=esm --bundle --target=chrome60,firefox60,safari12 --outfile=dist/move.esm.js --watch",
    "dev:demo": "./node_modules/.bin/browser-sync start -s -w --directory --startPath ./index.html",
    "build": "run-p build:*",
    "build:iife": "./node_modules/.bin/esbuild src/iife.js --format=iife --bundle --target=chrome60,firefox60,safari12 --outfile=dist/move.js",
    "build:iife-min": "./node_modules/.bin/esbuild src/iife.js --format=iife --bundle --minify --target=chrome60,firefox60,safari12 --outfile=dist/move.min.js",
    "build:cjs": "./node_modules/.bin/esbuild src/index.js --format=cjs  --bundle --target=chrome60,firefox60,safari12 --outfile=dist/move.cjs.js",
    "build:cjs-min": "./node_modules/.bin/esbuild src/index.js --format=cjs  --bundle --minify --target=chrome60,firefox60,safari12 --outfile=dist/move.cjs.min.js",
    "build:esm": "./node_modules/.bin/esbuild src/index.js --format=esm  --bundle --target=chrome60,firefox60,safari12 --outfile=dist/move.esm.js",
    "build:esm-min": "./node_modules/.bin/esbuild src/index.js --format=esm  --bundle --minify --target=chrome60,firefox60,safari12 --outfile=dist/move.esm.min.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "tween",
    "animation",
    "interpolation",
    "css",
    "dom",
    "fast",
    "lightweight",
    "tiny"
],
  "author": "Dmitrii Vasilev",
  "homepage": "move.js.org",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/caracal7/move.js.git"
  },
    "bugs": {
    "url": "https://github.com/caracal7/move.js/issues"
  },
  "homepage": "https://github.com/caracal7/move.js#readme",
  "devDependencies": {
    "browser-sync": "^3.0.2",
    "esbuild": "^0.20.1",
    "npm-run-all": "^4.1.5"
  }
}
